{
  "name": "modeling-errors-in-graphql-for-elm-apps",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node-dev --ignore-watch modeling-errors-typegen.ts --no-notify --transpileOnly --respawn ./src",
    "start-elm": "elm-live src/Main.elm --proxyPrefix /api --open --proxyHost http://localhost:4000",
    "regen-graphql": "elm-graphql http://localhost:4000/ --output gen"
  },
  "dependencies": {
    "apollo-server": "^2.2.0",
    "graphql": "^14.0.2",
    "nexus": "0.11.2",
    "ts-node-dev": "^1.0.0-pre.30"
  },
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^3.2.0",
    "elm-live": "^3.4.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  }
}
